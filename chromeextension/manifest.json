{
  "manifest_version": 2,
"name": "Youtube Transcript Summarizer",
"description": "This extension provises a summarized transcript of a Youtube Video!",
"version": "1.0",
"permissions": [
  "activeTab",
  "tabs",
  "http://127.0.0.1:8000/"
],
"background": {
  "scripts": ["background.js"],
  "persistent": false
},
"browser_action": {
  "default_icon": "extension_icon.png",
  "default_popup": "popup.html",
  "default_title": "YouTube Summarizer"
}
}
